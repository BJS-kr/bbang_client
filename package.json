{
  "name": "node6_final_server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon src/server.ts",
    "migrate": "drizzle-kit generate --config ./src/db/drizzle.config.ts && drizzle-kit migrate --config ./src/db/drizzle.config.ts",
    "push": "drizzle-kit push --config ./src/db/drizzle.config.ts",
    "generate:types": "pbjs -t static-module -w commonjs -o src/protobuf/compiled.js src/protobuf/game.proto && pbts -o src/protobuf/compiled.d.ts src/protobuf/compiled.js",
    "start:prod": "npx tsc && cp src/protobuf/compiled.js dist/protobuf/ && node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "drizzle-kit": "^0.26.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "protobufjs": "^7.4.0",
    "protobufjs-cli": "^1.1.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "long": "^5.2.3",
    "mysql2": "^3.11.3",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0"
  }
}
